<T3DataStructure>
	<sheets>
		<!--
			################################
			  SHEET General Settings
			################################
		-->
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:crowdfunding/Resources/Private/Language/locallang_flexform.xlf:flexform.tabSettings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<!-- PID Single Page -->
					<settings.detailPid>
						<TCEforms>
							<label>LLL:EXT:crowdfunding/Resources/Private/Language/locallang_flexform.xlf:flexform.detailPid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<wizards>
									<suggest>
										<type>suggest</type>
										<default>
											<searchWholePhrase>1</searchWholePhrase>
										</default>
									</suggest>
								</wizards>
							</config>
						</TCEforms>
					</settings.detailPid>

					<!-- PID Single Page -->
					<settings.listPid>
						<TCEforms>
							<label>LLL:EXT:crowdfunding/Resources/Private/Language/locallang_flexform.xlf:flexform.listPid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<wizards>
									<suggest>
										<type>suggest</type>
										<default>
											<searchWholePhrase>1</searchWholePhrase>
										</default>
									</suggest>
								</wizards>
							</config>
						</TCEforms>
					</settings.listPid>
				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>